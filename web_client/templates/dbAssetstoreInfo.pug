- var databaseInfo = assetstore.get('database') || {}
- var dbtypeNames = {mongo: 'MongoDB', sqlalchemy_mysql: 'MySQL', sqlalchemy_postgres: 'PostgreSQL', sqlalchemy_sqlite: 'SQLite', sqlalchemy: 'Generic SQL'}

div
  b Type:
  span.g-assetstore-type  Database
div
  b Database Type:
  span  #{dbtypeNames[databaseInfo.dbtype]}
div
  b Database URI:
  span  #{databaseInfo.uri.replace(/\/\/[^@\/]+@/, '//[user information]@')}
